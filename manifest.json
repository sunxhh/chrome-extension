{
  "manifest_version": 2,
  "name": "书签管理器",
  "description": "这是一个书签管理器",
  "version": "1.0",
  "background": {
    "persistent": true,
    "scripts": ["dist/background.js"]
  },
  "permissions": [
    "tabs",
    "storage",
    "bookmarks",
    "tabs",
    "activeTab",
    "<all_urls>",
    "history",
    "downloads",
    "chrome://favicon/",
    "chrome-search://thumb/"
  ],
  "browser_action": {
    "default_icon": {
      "48": "src/assets/img/icon.png"
    },
    "default_popup": "popup.html"
  }
}
